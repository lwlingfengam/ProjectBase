<div class="list-block" tapmode="highlight" onclick="openDetailWindow('<%= DeviceID %>','<%= Name %>');">
    <div class="list-block-header clearfix">
        <span class="title">
            <img class="icon" src="../image/home/nav/shape_nongye.png" alt=""><%= Name %>
        </span>
    </div>
    <div class="list-block-main">
        <div class="row">
            <div class="flex-1 text-left">实时流量：</div>
            <div class="flex-1 text-right"><em><%= Flow.toFixed(2) %></em>m³/s</div>
        </div>
        <div class="row">
            <div class="flex-1 text-left">今日总用水量：</div>
            <div class="flex-1 text-right"><em>--</em>m³</div>
        </div>
        <div class="row">
            <div class="flex-1 text-left">采集时间：</div>
            <div class="flex-1 text-right datetime"><%= Md %></div>
        </div>
    </div>
    <div class="line-dotted"></div>
    <div class="list-block-footer clearfix">
        <div class="btn pull-right">查看详情</div>
    </div>
</div>