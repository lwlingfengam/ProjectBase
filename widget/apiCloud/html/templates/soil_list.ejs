<div class="list-block" tapmode="highlight" onclick="openGroupFrames('<%= StationId %>', '<%= Name %>', '<%= Lng %>', '<%= Lat %>');">
    <div class="list-block-header clearfix">
        <span class="title">
            <img class="icon" src="../image/home/nav/shape_shangqing.png" alt=""><%= Name %>
        </span>
    </div>
    <div class="list-block-main">
        <div class="row">
            <div class="flex-1 text-left">站点位置:</div>
            <div class="flex-1 text-right"><%= Address %></div>
        </div>
    </div>
    <div class="line-dotted"></div>
    <div class="list-block-footer clearfix">
        <div class="btn pull-right">查看详情</div>
    </div>
</div>