<div class="list-block" tapmode="highlight" onclick="openGroupFrames('<%= IRGARCD %>','<%= IRGARNM %>');">
    <div class="list-block-header clearfix">
        <span class="title"><%= IRGARNM %></span>
    </div>
    <div class="list-block-main">
        <div class="row">
            <div class="flex-1 text-left">年度计划用水量:</div>
            <div class="flex-1 text-right"><em><%= Water_YearPlan %></em>万m³</div>
        </div>
        <div class="row">
            <div class="flex-1 text-left">年度已用水量:</div>
            <div class="flex-1 text-right"><em><%= Water_YearActual.toFixed(3) %></em>万m³</div>
        </div>
    </div>
    <div class="line-dotted"></div>
    <div class="list-block-footer clearfix">
        <div class="btn pull-right">查看详情</div>
    </div>
</div>